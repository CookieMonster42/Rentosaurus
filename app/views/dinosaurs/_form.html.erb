<div class="form">
  <%= simple_form_for @dinosaur do |f| %>
    <%= f.input :name %>
    <%= f.input :species %>
    <%#= f.input :age %>
    <%#= f.input :gender %>
    <%#= f.input :food %>
    <%= f.input :price %>
    <%= f.input :location %>
    <%#= f.input :policy %>
    <%= f.input :habits %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
  <div class = "button-position">
    <%= f.submit "Host your dino", class: 'btn btn-primary button-pink' %>
    <% end %>
    <p><%= link_to "Cancel", dinosaurs_path, class: "btn btn-primary link" %></p>
  </div>
</div>

